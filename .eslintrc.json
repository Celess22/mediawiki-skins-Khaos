{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"parserOptions": {
		"ecmaVersion": 2021
	},
	"env": {
		"browser": true,
		"jquery": true,
		"es6": true
	},
	"rules": {
		"computed-property-spacing": 0,
		"no-jquery/no-global-selector": 0,
		"space-before-function-paren": 0,
		"wrap-iife": 0
	}
}
